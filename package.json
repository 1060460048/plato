{
  "name": "PLATO",
  "version": "1.0.0",
  "description": ":heart: a Boilerplate for SPAs use vue, vuex-fsa, vue-router",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:mock": "better-npm-run dev -- --mock",
    "debug": "better-npm-run debug",
    "unit": "better-npm-run unit",
    "unit:dev": "npm run unit -- --watch",
    "e2e": "better-npm-run e2e",
    "test": "npm run lint && npm run unit",
    "lint": "eslint --max-warnings 10 .",
    "lint:fix": "npm run lint -- --fix",
    "clean": "better-npm-run clean",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "docs": "bash .tools/bash/docs.sh",
    "push": "bash .tools/bash/push.sh"
  },
  "betterScripts": {
    "compile": {
      "command": "node .tools/bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "start": {
      "command": "nodemon --harmony .tools/bin/start",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "dev": {
      "command": "nodemon --harmony .tools/bin/start",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "debug": {
      "command": "nodemon --harmony --debug .tools/bin/start",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "clean": {
      "command": "node .tools/bin/clean",
      "env": {
        "DEBUG": "koa*"
      }
    },
    "deploy": {
      "command": "npm test && npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "koa*"
      }
    },
    "unit": {
      "command": "node --harmony .tools/bin/unit",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "koa*"
      }
    },
    "e2e": {
      "command": "node --harmony .tools/bin/e2e",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "koa*"
      }
    }
  },
  "dependencies": {
    "chartist": "^0.9.8",
    "is-plain-obj": "^1.1.0",
    "nd-browser": "^3.0.0",
    "nd-datetime": "^3.0.5",
    "nd-promise": "^3.0.2",
    "plato-i18n": "^0.0.1",
    "plato-request": "^0.0.1",
    "plato-validator": "^0.0.1",
    "querystring": "^0.2.0",
    "string-template": "^1.0.0",
    "vue": "^1.0.25",
    "vue-router": "^0.7.13",
    "vuex-fsa": "^0.7.1",
    "vuex-localstorage": "^0.1.2",
    "vuex-promise": "^0.1.4",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.9",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "copy-webpack-plugin": "^3.0.0",
    "cross-spawn": "^4.0.0",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "github-markdown-css": "^2.3.0",
    "html-webpack-plugin": "^2.19.0",
